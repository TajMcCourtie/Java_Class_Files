public class Student {

	private String firstName;
	private String lastName;
	private String TRN;
	private String extracurricular;
	private String dateEnrolled;
	private int age;
	private String address;
	
	public Student(String firstName, String lastName, String TRN, String extracurricular, String dateEnrolled, int age, String address )
	{
		this.firstName = firstName;
		this.lastName = lastName;
		this.TRN = TRN;
		this.extracurricular = extracurricular;
		this.dateEnrolled = dateEnrolled;
		this.age = age;
		this.address = address;
	}
	public String getFirstName()
	{
		return this.firstName;
	}
	public void setFirstName(String firstName)
	{
		this.firstName = firstName;
	}
	public String getLastName() {
		return this.lastName;
	}
	public void setLastName(String lastName)
	{
		this.lastName = lastName;
	}
	public String getTRN()
	{
		return this.TRN;
		
	}
	public void setTRN(String TRN)
	{
		this.TRN = TRN;
	}
	public String getExtracurricular()
	{
		return this.extracurricular;
	}
	public void setExtracurricular(String extracurricular)
	{
		this.extracurricular = extracurricular;
	}
	public String getDateEnrolled()
	{
		return this.dateEnrolled;
	}
	public void setDateEnrolled(String dateEntrolled)
	{
		this.dateEnrolled = dateEnrolled;
	}
	public int getAge()
	{
		return this.age;
	}
	public void setAge(int age) {
		this.age = age;
	}
	public String getAddress()
	{
		return this.address;
	}
	public void setAddress(String address)
	{
		this.address = address;
	}
	
	
	
	public String toString()
	{
		return String.format("%s: %s %s \n%s, %s,%d, %s", 
				             "Student Name",firstName, lastName, 
				             "Student TRN", TRN, 
				             "Extracurricular", extracurricular, 
				             "Date Enrolled", dateEnrolled, 
				              "Student Age", age,
				              "Address", address);
	}
	
	public void determineExtracurricular() {
		
	}
	
}
